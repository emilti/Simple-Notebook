<div class="Absolute-Center is-Responsive">
        <form id="form-register">
        <input type="text" class="form-control register-username" id="tb-register-username" name="username" placeholder="Account name"
               required/>
        <input type="password" class="form-control" id="tb-register-passwordInitial" name="password" placeholder="Password"
               required/>
        <input type="password" class="form-control" id="tb-register-passwordConfirmed" name="password" placeholder="Confirm Password"
               required/>
        <button class="btn btn-lg btn-primary form-control" id="btn-register" type="submit">Register</button>
        <button class="btn btn-lg btn-primary form-control" id="btn-already-registered" onclick="return false;">I am registered. Go back to
            Log-In
        </button>
    </form>
</div>